    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "lint-staged": "^12.0.2",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
